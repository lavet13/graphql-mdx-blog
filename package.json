{
  "name": "apollo-prizma-vite-ts",
  "version": "1.0.0",
  "description": "Node JS project with Apollo and Prizma",
  "type": "module",
  "main": "dist/main.js",
  "private": "true",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vite build",
    "build:preview": "yarn build && yarn start:migrate:prod",
    "debug": "vite --debug --inspect-brk",
    "docker": "docker-compose up -d",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "start:migrate:prod": "npx prisma migrate deploy && yarn start:prod"
  },
  "keywords": [
    "graphql-yoga",
    "prizma",
    "vite",
    "typescript",
    "nodejs"
  ],
  "author": {
    "name": "Ivan Skinder",
    "email": "lavet13@mail.ru"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "tsx ./src/prisma/seed.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@parcel/watcher": "^2.4.0",
    "@swc/core": "^1.3.104",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "prisma": "^5.8.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-graphql-codegen": "^3.3.6",
    "vite-plugin-node": "^3.1.0",
    "vite-plugin-top-level-await": "^1.4.1"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@apollo/server-plugin-landing-page-graphql-playground": "^4.0.1",
    "@graphql-tools/load-files": "7.0.0",
    "@graphql-tools/merge": "^9.0.1",
    "@graphql-tools/resolvers-composition": "^7.0.0",
    "@graphql-tools/schema": "^10.0.2",
    "@graphql-yoga/plugin-jwt": "^2.3.0",
    "@graphql-yoga/plugin-response-cache": "^3.3.0",
    "@prisma/client": "^5.8.1",
    "@whatwg-node/server-plugin-cookies": "^1.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.4.2",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "tsx": "^4.10.4",
    "uuid": "^9.0.1"
  }
}
